<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_partner_supplies_report_pdf" model="ir.actions.report">
            <field name="name">Reporte de Productos Serializados (PDF)</field>
            <field name="model">partner.supplies.report.wizard</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">product_suppiles_partner.partner_supplies_report_pdf</field>
            <field name="report_file">product_suppiles_partner.partner_supplies_report_pdf</field>
            <field name="print_report_name">'Reporte_Productos_Serializados_%(partner_id.name)s'</field>
        </record>

        <record id="action_partner_supplies_report_xlsx" model="ir.actions.report">
            <field name="name">Reporte de Productos Serializados (Excel)</field>
            <field name="model">partner.supplies.report.wizard</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">product_suppiles_partner.partner_supplies_report_xlsx</field>
            <field name="report_file">product_suppiles_partner.partner_supplies_report_xlsx</field>
            <field name="binding_model_id" ref="base.model_res_partner"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>

