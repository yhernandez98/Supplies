<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_inherit_license" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.license</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <div class="app_settings_block" data-string="Licenciamientos" string="Licenciamientos" data-key="subscription_licenses">
                    <h2>Licenciamientos</h2>
                </div>
            </xpath>
        </field>
    </record>
</odoo>

