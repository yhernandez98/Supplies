<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_check_expired_licenses" model="ir.cron">
        <field name="name">Verificar Licencias Vencidas</field>
        <field name="model_id" ref="model_license_assignment"/>
        <field name="state">code</field>
        <field name="code">model._cron_check_expired_licenses()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="True"/>
    </record>

</odoo>

